{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "outDir": "api/_engine",
    "rootDir": "src",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "noEmitOnError": false
  },
  "include": [
    "src/engine/**/*.ts",
    "src/serverSim.ts"
  ],
  "exclude": [
    "src/engine/__tests__/**"
  ]
}
